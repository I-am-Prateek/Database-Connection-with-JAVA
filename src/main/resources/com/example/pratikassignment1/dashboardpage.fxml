<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="552.0" prefWidth="663.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pratikassignment1.crudecontroller">
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </padding>
   <children>
      <AnchorPane prefHeight="287.0" prefWidth="572.0">
         <children>
            <Button fx:id="user_btn" layoutX="197.0" layoutY="172.0" mnemonicParsing="false" onAction="#myuserlogin" prefHeight="28.0" prefWidth="67.0" style="-fx-background-color: #5AB0F7; -fx-background-radius: 15; -fx-cursor: hand;" text="User" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
            <Button fx:id="logout_btn" layoutX="350.0" layoutY="172.0" mnemonicParsing="false" onAction="#myuserlogout" style="-fx-background-color: #5AB0F7; -fx-background-radius: 15; -fx-cursor: hand;" text="Log Out" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
            <ImageView fitHeight="39.0" fitWidth="66.0" layoutX="211.0" layoutY="124.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/user.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="45.0" fitWidth="45.0" layoutX="364.0" layoutY="118.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/logout.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
   

</VBox>
